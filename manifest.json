{
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
        "default_icon": "img/icon-active.png",
        "default_title": "AllTabGroups"
    },
    "content_scripts": [
        {
            "js": [
                "js/vendor/jquery-2.2.4.min.js"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "description": "Manage tabs. Instant save, export and import tabs from certain groups called either as default color or custom title.",
    "icons": {
        "128": "img/icon128.png",
        "16": "img/icon16.png",
        "48": "img/icon48.png"
    },
    "manifest_version": 3,
    "name": "AllTabGroups",
    "options_page": "options.html",
    "permissions": [
        "tabGroups",
        "tabs",
        "storage",
        "downloads",
        "bookmarks"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "version": "1.0"
}
